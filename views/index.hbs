<div class="movie">
     <h1>Movie List</h1> 
      <ul class = "movie-info">
            {{#each movie}}
            
            <li><h2><a href="/movies/oneMovie/{{this._id}}">{{this.title}}</a></h2>
            <form action = "/movies/delete/{{this._id}}" method="post">
    <input type="submit" name="" value ="Delete"> <a href="/movies/edit/{{this._id}}">Edit</a>
</form></li>
            {{/each}}
        </ul>
</div>
