{{#each theList}}
    <h3><a href="/movies/select/{{this._id}}">  {{this.title}} </a></h3>
    <p>{{this.director}}</p>
    <h6>{{this.year}}</h6>
    <a href="/movies/edit/{{this._id}}">  Edit   </a>
    <form  action="/movies/delete/{{this._id}}" method="post">
        <button type="submit"> Delete </button>
    </form>
{{/each}}
