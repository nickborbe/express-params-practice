<a href="/movies">Return to full list of movies</a>
<br>
<br>

{{#each movie}}
<h2> {{this.title}} </h2>
<h4> Director: {{this.director}} </h4>
<h4> Year: {{this.year}} </h4>
<h4> Rating: {{this.rate}} </h4>
<h4> Duration: {{this.duration}}</h4>
<h4> Genre(s):</h4>
{{#each this.genre}}
<ul>
  <li>{{this}}</li>
</ul>
{{/each}}
<br>
{{/each}}